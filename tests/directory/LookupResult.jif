class LookupResult {
    final label{this} label_; // XXX decl removed in Fabric version
	final Object{*label_} obj;
    boolean {*label_} found;

    LookupResult(label{this} l, Object{*l} o) {
		label_ = l;
        obj = o;
        found = (o == null) ? false : true;
    }
    /*
    LookupResult(label{this} l) {
		label_ = l;
        obj = null;
        found = false;
    }
    */
}

