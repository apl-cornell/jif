package util;

public class Util {

  /**
   * Copy the contents of an array into another array with the given labels.
   */
  public static
  char{l}[]{m}
  copyArray
  (
    char{n}[]{o} data,
    label l,
    label m,
    label n,
    label o,
    Store s
  )
  where
    n <= l,
    o <= m
  {
    char{l}[]{m} result = new char[data.length];
    for (int i = 0; i < data.length; i++)
      result[i] = data[i];
    return result;
  }
}


