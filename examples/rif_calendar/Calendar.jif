	public void publicizeSlot(Event e){
		rifList el = this.eventList;
		if (el != null && e != null){
			Event newe = new Event(e.getDate(), e.getTime(), e.getDuration(), e.getDescription(), e.getCreator());
			newe.setPubSlot(true);
			el.remove(e);
			el.add(newe);
		}
	}

